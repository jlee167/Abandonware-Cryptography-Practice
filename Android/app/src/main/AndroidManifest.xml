<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jihoonlee.myapplication">

    <application
        android:name=".ImageCryptApp"
        android:allowBackup="true"
        android:icon="@drawable/chaos"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:largeHeap="true">
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EncryptActivity" />
        <activity android:name=".DecryptActivity" />
        <activity android:name=".CloudActivity" />
        <activity android:name=".PasswordActivity"></activity>
    </application>

</manifest>